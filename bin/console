#!/usr/bin/env php
<?php

require(__DIR__ . '/../vendor/autoload.php');

use Symfony\Component\Console\Application;
use Contao\Connector\Console\ConfigAllCommand;
use Contao\Connector\Console\ConfigDisableCommand;
use Contao\Connector\Console\ConfigEnableCommand;
use Contao\Connector\Console\ConfigGetCommand;
use Contao\Connector\Console\ConfigRemoveCommand;
use Contao\Connector\Console\ConfigSetCommand;
use Contao\Connector\Console\LogsFilesCommand;
use Contao\Connector\Console\LogsReadCommand;
use Contao\Connector\Console\StatusCommand;
use Contao\Connector\Console\SyslogEntriesCommand;
use Contao\Connector\Console\UserInfoCommand;
use Contao\Connector\Console\UserResetPasswordCommand;

$application = new Application();
$application->add(new ConfigAllCommand);
$application->add(new ConfigDisableCommand);
$application->add(new ConfigEnableCommand);
$application->add(new ConfigGetCommand);
$application->add(new ConfigRemoveCommand);
$application->add(new ConfigSetCommand);
$application->add(new LogsFilesCommand);
$application->add(new LogsReadCommand);
$application->add(new StatusCommand);
$application->add(new SyslogEntriesCommand);
$application->add(new UserInfoCommand);
$application->add(new UserResetPasswordCommand);
$application->run();
